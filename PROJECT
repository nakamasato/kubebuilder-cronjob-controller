domain: tutorial.kubebuilder.io
layout:
- go.kubebuilder.io/v3
projectName: kubebuilder-cronjob-controller
repo: tutorial.kubebuilder.io
resources:
- api:
    crdVersion: v1
    namespaced: true
  controller: true
  domain: tutorial.kubebuilder.io
  group: batch
  kind: CronJob
  path: tutorial.kubebuilder.io/api/v1
  version: v1
  webhooks:
    defaulting: true
    validation: true
    webhookVersion: v1
- api:
    crdVersion: v1
    namespaced: true
  domain: tutorial.kubebuilder.io
  group: batch
  kind: CronJob
  path: tutorial.kubebuilder.io/api/v2
  version: v2
  webhooks:
    conversion: true
    webhookVersion: v1
version: "3"
